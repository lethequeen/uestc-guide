/**
 * 公共样式
 */
.clearfix
	position: relative
	&:after
		display: block
		content: ''
		height: 0
		clear: both
		visibility: hidden
		font-size: 0

html, body
	font-size: 14px
li
	list-style: none
.f-left
	float: left
.f-right
	float: right
	

